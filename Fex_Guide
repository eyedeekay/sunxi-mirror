uart_port = 3
uart_type = 4
uart_tx = port:PH00&lt;4&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
uart_rx = port:PH01&lt;4&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
uart_rts = port:PH02&lt;4&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
uart_cts = port:PH03&lt;4&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
</pre>

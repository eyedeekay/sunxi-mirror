uart_rx = port:PI19&lt;3&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
uart_rts = port:PI16&lt;3&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
uart_cts = port:PI17&lt;3&gt;&lt;1&gt;&lt;default&gt;&lt;default&gt;
</pre>

csi_d15 = port:PG11&lt;5&gt;&lt;default&gt;&lt;default&gt;&lt;default&gt;
csi_reset = port:PH13&lt;1&gt;&lt;default&gt;&lt;default&gt;&lt;0&gt;
csi_power_en = port:PH16&lt;1&gt;&lt;default&gt;&lt;default&gt;&lt;0&gt;
csi_stby = port:PH18&lt;1&gt;&lt;default&gt;&lt;default&gt;&lt;0&gt;
csi_flash = 
csi_af_en = 
